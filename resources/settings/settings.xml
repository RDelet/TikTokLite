            <listEntry value="1">@Strings.ColorRed</listEntry>
            <listEntry value="2">@Strings.ColorDarkRed</listEntry>
            <listEntry value="3">@Strings.ColorOrange</listEntry>
            <listEntry value="4">@Strings.ColorYellow</listEntry>
            <listEntry value="5">@Strings.ColorBlue</listEntry>
            <listEntry value="6">@Strings.ColorDarkBlue</listEntry>
            <listEntry value="7">@Strings.ColorGreen</listEntry>
            <listEntry value="8">@Strings.ColorDarkGreen</listEntry>
            <listEntry value="9">@Strings.ColorPurple</listEntry>
            <listEntry value="10">@Strings.ColorPink</listEntry>
        </settingConfig>
    </setting>

